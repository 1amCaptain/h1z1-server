    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch ZoneServer",
            "env":{
                "VSCODE_DEBUG":"true"
            },
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/servers/ZoneServer/zoneserver.ts",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch LoginServer",
            "env":{
                "VSCODE_DEBUG":"true"
            },
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/servers/LoginServer/loginserver.ts",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ]
        }
    ]
}